	keywords = {generalized estimating equations, linear mixed effects models, longitudinal data, repeated measures ANOVA, small sample},
	pages = {64--87},
	file = {SAGE PDF Full Text:/Users/kels/Zotero/storage/GKM24HSH/Muth et al. - 2016 - Alternative Models for Small Samples in Psychologi.pdf:application/pdf}
