	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/1469-8986.3710013},
	keywords = {Mixed effects models, Repeated measures designs, Variance-covariance matrix},
	pages = {13--20},
	file = {Full Text PDF:/Users/kels/Zotero/storage/PUIBZWRY/Bagiella et al. - 2000 - Mixed-effects models in psychophysiology.pdf:application/pdf;Snapshot:/Users/kels/Zotero/storage/VZYT5F3A/1469-8986.html:text/html}
}

@article{muthAlternativeModelsSmall2016,
